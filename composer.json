{
  "name": "avanza-git/cas",
  "description": "Adds CAS support to Laravel 5.x - 11.x",
  "keywords": [
    "CAS",
    "phpCAS",
    "SSO",
    "laravel",
    "laravel 6",
    "laravel 7",
    "laravel 8",
    "laravel 9",
    "laravel 10",
    "laravel 11",
    "laravel 12"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "avanza-git",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "apereo/phpcas": "^1.6",
    "monolog/monolog": "^2.0|^3.0"
  },
  "require-dev": {
   "orchestra/testbench": "^6.25",
    "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0",
    "pestphp/pest": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "AvanzaGit\\LaravelCas\\": "src/AvanzaGit/LaravelCas"
    },
    "files": [
      "src/AvanzaGit/LaravelCas/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AvanzaGit\\LaravelCas\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "AvanzaGit\\LaravelCas\\CasServiceProvider"
      ],
      "aliases": {
        "Cas": "AvanzaGit\\LaravelCas\\Facades\\Cas"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
